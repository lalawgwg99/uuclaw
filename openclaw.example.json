{
  "env": {
    "OPENROUTER_API_KEY": "sk-or-v1-YOUR_KEY",
    "BRAVE_API_KEY": "YOUR_BRAVE_KEY",
    "RETRY_LIMIT": "5"
  },
  "agents": {
    "defaults": {
      "model": { "primary": "openrouter/minimax/minimax-m2.5" },
      "imageModel": { "primary": "openrouter/google/gemini-2.0-flash-exp" },
      "models": {
        "openrouter/minimax/minimax-m2.5": { "alias": "MiniMax" },
        "openrouter/deepseek/deepseek-v3.2": { "alias": "DeepSeek V3.2" },
        "openrouter/arcee-ai/trinity-large-preview:free": { "alias": "Trinity Free" },
        "openrouter/stepfun/step-3.5-flash:free": { "alias": "Step Flash Free" },
        "openrouter/google/gemini-2.0-flash-exp": { "alias": "Gemini Vision" }
      },
      "workspace": "/path/to/your/workspace",
      "compaction": { "mode": "safeguard" }
    }
  },
  "commands": { "native": "auto", "nativeSkills": "auto", "restart": true },
  "channels": {
    "telegram": {
      "enabled": true,
      "dmPolicy": "pairing",
      "botToken": "YOUR_BOT_TOKEN",
      "groupPolicy": "allowlist",
      "streamMode": "partial",
      "reactionLevel": "ack"
    }
  },
  "gateway": {
    "mode": "local",
    "bind": "loopback",
    "auth": { "mode": "token", "token": "GENERATE_OR_USE_OPENCLAW_DEFAULT" }
  },
  "plugins": { "entries": { "telegram": { "enabled": true } } }
}
